<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <changeSet id="add-cat-type-constr" author="Maks">
        <addForeignKeyConstraint baseTableName="category_type" baseColumnNames="category_id"
                                 constraintName="fk-cat-type" referencedTableName="category"
                                 referencedColumnNames="category_id"></addForeignKeyConstraint>
        <addForeignKeyConstraint baseTableName="category_type" baseColumnNames="type_id" constraintName="fk-type-cat"
                                 referencedTableName="type"
                                 referencedColumnNames="type_id"></addForeignKeyConstraint>
    </changeSet>


</databaseChangeLog>